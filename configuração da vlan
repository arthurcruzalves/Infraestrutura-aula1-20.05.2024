enable
  show vlan brief
  configure terminal
     vlan 10
         name switch
         exit
     vlan 20
         name serve 
         exit
     vlan 30
         name wireless
         exit 
     vlan 40 
         name estoque
         exit   
     vlan 50
         name financeiro
         exit
     vlan 60
         name gerencia
     end
write     
show vlan brief

enable
    configure terminal
       interface vlan 10 
         description Interface SVI Switch 3650
         ip address 192.168.10.252 255.255.255.0
         no shutdown
         end 
    write         
    show ip interface brief 
  
    
